<h1>Listing Game transcripts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Image</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for game_transcript <- @game_transcripts do %>
    <tr>
      <td><%= game_transcript.title %></td>
      <td><%= game_transcript.image %></td>

      <td>
        <span><%= link "Chapters", to: Routes.admin_game_transcript_chapter_path(@conn, :index, game_transcript) %></span>
        <span><%= link "Show", to: Routes.admin_game_transcript_path(@conn, :show, game_transcript) %></span>
        <span><%= link "Edit", to: Routes.admin_game_transcript_path(@conn, :edit, game_transcript) %></span>
        <span><%= link "Delete", to: Routes.admin_game_transcript_path(@conn, :delete, game_transcript), method: :delete, data: [confirm: "Are you sure?"] %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Game transcript", to: Routes.admin_game_transcript_path(@conn, :new) %></span>
